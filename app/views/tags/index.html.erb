<h1>Look at these tags!
<%= link_to "  Add a new tag!", new_tag_path, class: 'btn btn-primary btn-xs glyphicon glyphicon-plus pull-right' %>
</h1>
<% @tags.each do |tag| %>
		<div class="well">
		  <h2><%= tag.name %></h2>
		  <%= link_to "", tag, method: :delete, class: 'btn btn-danger btn-xs glyphicon glyphicon-trash pull-right' %>
		  <%= link_to "checkout this tag", tag, class: 'pull-right' %>

		</div>
<% end %>